apiVersion: v1
clusters:
- cluster:
    certificate-authority-data: LQo=
    server: https://ABC.amazonaws.com
  name: kubernetes
contexts:
- context:
    cluster: kubernetes
    user: aws
  name: aws
current-context: aws
kind: Config
preferences: {}
users:
- name: aws
  user:
    exec:
      apiVersion: client.authentication.k8s.io/v1alpha1
      args:
      - token
      - -i
      - exberry-two
      command: aws-iam-authenticator